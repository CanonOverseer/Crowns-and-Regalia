<?xml version="1.0" encoding="utf-8" ?>
<Defs>

  <ThingDef Name="Canon_Crown_BaseThing" ParentName="ApparelBase" Abstract="True">
    <relicChance>1.2</relicChance>

  </ThingDef>


  <ThingDef ParentName="Canon_Crown_BaseThing">
    <defName>Canon_Crown_Byzantine</defName>
    <label>Byzantine Imperial Crown</label>
    <description>A crown of metal with an elaborate design, truly worthy of a successor to Rome!</description>
    <smeltable>true</smeltable>
    <thingCategories>
      <li>HeadgearNoble</li>
    </thingCategories>
    <recipeMaker>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Smith</effectWorking>
      <soundWorking>Recipe_Smith</soundWorking>
      <recipeUsers>
        <li>ElectricSmithy</li>
        <li>FueledSmithy</li>
      </recipeUsers>
      <researchPrerequisite>Smithing</researchPrerequisite>
    </recipeMaker>
    <graphicData>
      <texPath>Apparel/Headgear/crown1</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>(0.9, 0.9)</drawSize>
    </graphicData>
    <costList>
      <Gold>40</Gold>
      <Cloth>30</Cloth>
    </costList>
    <statBases>
      <MaxHitPoints>80</MaxHitPoints>
      <WorkToMake> 24000</WorkToMake>
      <Mass>0.9</Mass>
      <EquipDelay>0.8</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <SocialImpact>0.3</SocialImpact>
      <SuppressionPower MayRequire="Ludeon.RimWorld.Ideology">0.15</SuppressionPower>
      <SlaveSuppressionOffset MayRequire="Ludeon.RimWorld.Ideology">-0.3</SlaveSuppressionOffset>
    </equippedStatOffsets>
    <apparel>
      <canBeDesiredForIdeo>true</canBeDesiredForIdeo>
      <countsAsClothingForNudity>false</countsAsClothingForNudity>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Apparel/Headgear/crown1</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>Royal</li>
        <li>RoyalTier7</li>
      </tags>
      <canBeGeneratedToSatisfyWarmth>false</canBeGeneratedToSatisfyWarmth>
    </apparel>
  </ThingDef>

